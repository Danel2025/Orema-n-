{
  "name": "gabon-pos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:clean": "node scripts/clean-next.mjs && next dev --turbopack",
    "clean": "node scripts/clean-next.mjs",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
